  </servlet>
  <servlet-mapping>
    <servlet-name>ListHandler</servlet-name>
    <url-pattern>/ListHandler</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>JobsHandler</display-name>
