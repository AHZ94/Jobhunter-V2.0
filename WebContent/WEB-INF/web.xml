  </servlet>
  <servlet-mapping>
    <servlet-name>JobsHandler</servlet-name>
    <url-pattern>/JobsHandler</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RatingHandler</display-name>
    <servlet-name>RatingHandler</servlet-name>
    <servlet-class>com.rating.RatingHandler</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RatingHandler</servlet-name>
    <url-pattern>/RatingHandler</url-pattern>
  </servlet-mapping>
</web-app>