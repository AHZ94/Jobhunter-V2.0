    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>LoginEmployer</display-name>
    <servlet-name>LoginEmployer</servlet-name>
    <servlet-class>com.employer.LoginEmployer</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginEmployer</servlet-name>
    <url-pattern>/LoginEmployer</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>EmployerHandler</display-name>
    <servlet-name>EmployerHandler</servlet-name>
